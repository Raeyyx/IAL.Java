package class_i;

public class Class_i {
    public static void main(String[] args) {
        int a = 15;
        int b = 27;
        int c = 10;
        
        int max = findMax(a, b, c);
        System.out.println("Der größte Wert ist: " + max);
    }
    
    public static int findMax(int a, int b, int c) {
        int max = a; // Ausgangswert
        if (b > max) {
            max = b;
        }
        if (c > max) {
            max = c;
        }
        return max;
    }
}
